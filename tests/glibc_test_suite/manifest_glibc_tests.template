=====================================================================
== switches for zerovm. some of them used to control nexe, some
== for the internal zerovm needs
=====================================================================
Version = 09082012
Nexe = {OUTFILE}.nexe
==MemMax=134217728
MemMax={MEMMAX}
Timeout=10
Environment = VERBOSITY, 2, TimeStamp, 1344719070 {ENVIRONMENT}, LOCPATH, /lib/locale
NodeName=glibc-test,1

