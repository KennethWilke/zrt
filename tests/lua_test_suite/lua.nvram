###########################################################
# ZRT fstab file, injects tar images contents into ZRT FS #
###########################################################
[fstab]
channel=/dev/mount/root, mountpoint=/, access=ro, removable=no

[args]
args=lua.nexe -e_U=true /lua-5.2.1-tests/all.lua

[env]
name=PATH, value=foo

[debug]
verbosity=3
